{
    "env": {
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "script",
      "ecmaFeatures": {
        "impliedStrict": true,
      }
    },
    "rules": {
        "strict": [ 1, "global" ],
        "indent": [ "error", 2 ],
        "linebreak-style": [ 1, "unix" ],
        "quotes": [ 0, "single" ],
        "semi": [ 2, "always" ],
        "curly": [ 0, "multi", "consistent" ],
        "no-console": 0,
        "no-unused-vars": [ 1, {
            "args": "after-used",
            "argsIgnorePattern": "^_",
            "varsIgnorePattern": "^_"
        }]
    }
}
